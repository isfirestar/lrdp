{
    "mainloop" : {
        "module" : "libnav.so",
        "preinitproc" : "nav_entry",
        "contextsize" : 32
    },

    "redis-server" : {
        "mysub" : {
            "host" : "127.0.0.1:7777",
            "contextsize" : 0
        },

        "mypub" : {
            "host" : "127.0.0.1:7777",
            "contextsize" : 0
        }
    },

    "timers" : {
        "nav_traject_control" : {
            "module" : "libnav.so",
            "interval" : 1000,
            "timerproc" : "nav_traject_control",
            "contextsize" : 40
        }
    },

    "subscriber" : {
        "velocity" : {
            "redis-server" : "mysub",
            "module" : "libnav.so",
            "execproc" : "nav_on_velocity_feedback",
            "channels" : [
                "feedback.velocity.?*",
                "feedback.v_?"
            ]
        }
    },

    "imports" : {
        "publisher" : {
            "module" : "libpublisher.so",
            "writeproc" : "nav_publish_velocity",
            "vwriteproc" : "nav_publish_any",
            "contextsize" : 0
        }
    }
}

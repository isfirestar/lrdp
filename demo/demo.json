{
    "mainloop" : {
        "module" : "libdemo.so",
        "preinitproc" : "dep_pre_init",
        "postinitproc" : "dep_post_init",
        "exitproc" : "dep_atexit",
        "contextsize" : 128,
        "entrytimer" : {
            "interval" : 1000,
            "timerproc" : "dep_on_timer"
        }
    },

    "lwps" : {
        "demobg" : {
            "module" : "libdemo.so",
            "execproc" : "dep_bg_exec",
            "stacksize" : 1024,
            "priority" : 0,
            "contextsize" : 128,
            "affinity" : 0
        }
    },

    "nets" : {
        "testsrv" : {
            "protocol" : "tcp",
            "module" : "libdemo.so",
            "listen" : "0.0.0.0:10256",
            "acceptproc" : "dep_tcp_on_accept",
            "recvproc" : "dep_tcp_on_received",
            "closeproc" : "dep_tcp_on_closed",
            "contextsize" : 16
        },

        "testcli" : {
            "protocol" : "tcp",
            "module" : "libdemo.so",
            "local" : "0.0.0.0:10255",
            "remote" : "127.0.0.1:10256",
            "connectproc" : "dep_tcp_on_connected",
            "recvproc" : "dep_tcp_on_received",
            "closeproc" : "dep_tcp_on_closed"
        }
    }
}

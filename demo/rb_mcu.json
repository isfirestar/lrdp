{
    "mainloop" : {
        "module" : "/mnt/d/git/my/lrdp/gmake/build/bin/librbmcu.so",
        "preinitproc" : "rb_mcu_init",
        "contextsize" : 32
    },

    "lwps" : {
        "updatev" : {
            "module" : "/mnt/d/git/my/lrdp/gmake/build/bin/librbmcu.so",
            "execproc" : "rb_mcu_on_velocity_update",
            "stacksize" : 1024,
            "priority" : 0,
            "contextsize" : 0,
            "affinity" : 0
        }
    },

    "ttys" : {
        "motionmcu" : {
            "module" : "/mnt/d/git/my/lrdp/gmake/build/bin/librbmcu.so",
            "device" : "/dev/uart1",
            "baudrate" : 38400,
            "databits" : 8,
            "stopbits" : 1,
            "parity" : "none",
            "flowcontrol" : "none",
            "recvproc" : "rb_mcu_on_recvdata",
            "contextsize" : 0
        }
    },

    "redis-server" : {
        "myrobot" : {
            "host" : "127.0.0.1:6379",
            "contextsize" : 0
        }
    },

    "subscribe" : {
        "velocity" : {
            "redis-server" : "myrobot",
            "module" : "/mnt/d/git/my/lrdp/gmake/build/bin/librbmcu.so",
            "execproc" : "rb_mcu_on_velocity_update",
            "pattern" : [
                "motion.velocity.?*",
                "motion.v_?"
            ]
        }
    }
}

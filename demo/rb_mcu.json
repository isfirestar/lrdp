{
    "mainloop" : {
        "module" : "/mnt/d/git/my/lrdp/gmake/librbmcu.so",
        "preinitproc" : "rb_mcu_init",
        "contextsize" : 32
    },

    "ttys" : {
        "rb_motion_tty" : {
            "module" : "/mnt/d/git/my/lrdp/gmake/librbmcu.so",
            "device" : "/dev/uart1",
            "baudrate" : 38400,
            "databits" : 8,
            "stopbits" : 1,
            "parity" : "none",
            "flowcontrol" : "none",
            "recvproc" : "rb_mcu_on_recvdata",
            "contextsize" : 0
        }
    },

    "redis-server" : {
        "mysub" : {
            "host" : "127.0.0.1:7777",
            "contextsize" : 0
        },

        "mypub" : {
            "host" : "127.0.0.1:7777",
            "contextsize" : 0
        }
    },

    "subscriber" : {
        "velocity" : {
            "redis-server" : "mysub",
            "module" : "/mnt/d/git/my/lrdp/gmake/librbmcu.so",
            "execproc" : "rb_mcu_on_velocity_update",
            "channels" : [
                "motion.velocity.?*",
                "motion.v_?"
            ]
        }
    },

    "imports" : {
        "publisher" : {
            "module" : "/mnt/d/git/my/lrdp/gmake/libpublisher.so",
            "writeproc" : "nav_publish_velocity",
            "vwriteproc" : "nav_publish_any",
            "contextsize" : 0
        }
    }
}
